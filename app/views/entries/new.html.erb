<h1>Create new entry</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@entry, class:"form", html:{role:"form"}) do |f| %>
      <%= render 'fields', f: f %>
      <div class="form-group">
         <%= f.submit "Create entry", class: "btn btn-primary btn-lg"%>
    </div>
    <% end %>
  </div>
</div>